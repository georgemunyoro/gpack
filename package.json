{
  "name": "gpack",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "description": "A package manager for nodejs projects",
  "author": "George Munyoro",
  "bin": {
    "gpack": "./dist/index.js"
  },
  "scripts": {
    "test-script": "dd",
    "build": "tsc",
    "install-gpack": "yarn global add file:$PWD",
