    "build-install": "yarn clean && yarn build && yarn install-gpack"
  },
  "dependencies": {
    "@types/command-exists": "^1.2.0",
    "command-exists": "^1.2.9",
    "commander": "^10.0.1",
    "figlet": "^1.6.0"
  },
  "devDependencies": {
    "@types/figlet": "^1.5.6",
    "@types/node": "^18.16.3",
    "typescript": "^5.0.4"
  }
}
